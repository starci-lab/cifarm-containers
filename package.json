    "testRegex": "apps/.*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "@swc/jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "moduleNameMapper": {
      "^@src/(.*)": "<rootDir>/src/$1",
      "^@apps/(.*)": "<rootDir>/apps/$1"
    },
