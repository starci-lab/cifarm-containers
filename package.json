    "@solana/web3.js": "^1.95.4",
    "@telegram-apps/init-data-node": "^1.1.1",
    "@types/bs58": "4.0.4",
    "@unique-nft/sdk": "^2.1.2",
    "@unique-nft/sr25519": "^0.0.2",
    "algosdk": "^3.0.0",
    "bip39": "3.1.0",
    "cache-manager": "5.7.6",
    "cache-manager-redis-yet": "5.1.5",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "ethers": "6.13.4",
    "multiformats": "9.9.0",
    "near-api-js": "5.0.1",
