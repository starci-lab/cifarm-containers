  "scripts": {
    "build": "nest build",
    "build:docker": "docker build -t cifarm/cli -f ./apps/cli/Dockerfile . & docker push cifarm/cli & docker build -t cifarm/gameplay-subgraph -f ./apps/gameplay-subgraph/Dockerfile . & docker push cifarm/gameplay-subgraph & docker build -t cifarm/graphql-gateway -f ./apps/graphql-gateway/Dockerfile . & docker push cifarm/graphql-gateway & docker build -t cifarm/cron-worker -f ./apps/cron-worker/Dockerfile . & docker push cifarm/cron-worker & docker build -t cifarm/rest-api-gateway -f ./apps/rest-api-gateway/Dockerfile . & docker push cifarm/rest-api-gateway & docker build -t cifarm/io -f ./apps/io/Dockerfile . & docker push cifarm/io",
    "cli:dev": "nest build cli && node dist/apps/cli/main.js",
    "cli": "node dist/apps/cli/main.js",
    "format": "prettier --write \"apps/**/*.ts\" ",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:gameplay": "concurrently \"nest start ws --watch\" \"nest start cron-scheduler --watch\" \"nest start cron-worker --watch\" \"nest start gameplay-subgraph --watch\" \"nest start graphql-gateway --watch\" \"nest start social-auth --watch\"",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/cifarm-server/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:e2e": "jest --config ./e2e/jest.json",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "lint-staged": "lint-staged"
  },
