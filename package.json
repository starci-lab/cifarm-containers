  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": "apps/.*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "moduleNameMapper": {
      "^@src/(.*)": "<rootDir>/src/$1",
      "^@apps/(.*)": "<rootDir>/apps/$1"
    },
    "collectCoverage": false,
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/"
    ]
  },
  "lint-staged": {
    "*.ts": [
      "npm run format",
      "npm run lint",
      "git add ."
