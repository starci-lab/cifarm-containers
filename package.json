    "@nestjs/core": "^10.0.0",
    "@nestjs/graphql": "12.2.1",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/microservices": "10.4.7",
    "@nestjs/passport": "10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "10.4.7",
