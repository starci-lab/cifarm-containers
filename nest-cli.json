{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/cifarm-server/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true
  },
  "monorepo": true,
  "root": "apps/cifarm-server",
  "projects": {
    "healthcheck-service": {
      "type": "application",
      "root": "apps/healthcheck-service",
      "entryFile": "../dist/apps/healthcheck-service/main",
      "sourceRoot": "apps/healthcheck-service/src",
      "compilerOptions": {
        "webpack": true,
        "webpackConfigPath": "apps/healthcheck-service/webpack.config.js"
      }
    },
    "rest-api-gateway": {
      "type": "application",
      "root": "apps/rest-api-gateway",
      "entryFile": "../dist/apps/rest-api-gateway/main",
      "sourceRoot": "apps/rest-api-gateway/src",
      "compilerOptions": {
        "webpack": true,
        "webpackConfigPath": "apps/rest-api-gateway/webpack.config.js"
      }
    },
    "websocket-node": {
      "type": "application",
      "root": "apps/websocket-node",
      "entryFile": "../dist/apps/websocket-node/main",
      "sourceRoot": "apps/websocket-node/src",
      "compilerOptions": {
        "webpack": true,
        "webpackConfigPath": "apps/websocket-node/webpack.config.js"
      }
    },
    "graphql-maingraph": {
      "type": "application",
      "root": "apps/graphql-maingraph",
      "entryFile": "../dist/apps/graphql-maingraph/main",
      "sourceRoot": "apps/graphql-maingraph/src",
      "compilerOptions": {
        "webpack": true,
        "webpackConfigPath": "apps/graphql-maingraph/webpack.config.js"
      }
    },
    "cron-scheduler": {
      "type": "application",
      "root": "apps/cron-scheduler",
      "entryFile": "../dist/apps/cron-scheduler/main",
      "sourceRoot": "apps/cron-scheduler/src",
      "compilerOptions": {
        "webpack": true,
        "webpackConfigPath": "apps/cron-scheduler/webpack.config.js"
      }
    },
    "cron-worker": {
      "type": "application",
      "root": "apps/cron-worker",
      "entryFile": "../dist/apps/cron-worker/main",
      "sourceRoot": "apps/cron-worker/src",
      "compilerOptions": {
        "webpack": true,
        "webpackConfigPath": "apps/cron-worker/webpack.config.js"
      }
    },
    "static-subgraph": {
