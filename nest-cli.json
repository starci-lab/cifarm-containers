      "root": "apps/gameplay-service",
      "entryFile": "../dist/apps/gameplay-service/main",
      "sourceRoot": "apps/gameplay-service/src",
      "compilerOptions": {
        "webpack": true,
        "webpackConfigPath": "apps/gameplay-service/webpack.config.js"
      }
    },
    "seed": {
      "type": "application",
      "root": "apps/seed",
      "entryFile": "../dist/apps/seed/main",
      "sourceRoot": "apps/seed/src",
      "compilerOptions": {
        "webpack": true,
        "webpackConfigPath": "apps/seed/webpack.config.js"
      }
    }
  }
}