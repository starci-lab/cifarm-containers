podTemplate(inheritFrom: 'default', serviceAccount: 'jenkins')
{
    node(POD_LABEL){
      stage('List Configmaps') {
        withAWS(credentials: 'aws-credentials', region: 'ap-southeast-1') {
                        sh 'aws eks update-kubeconfig --name "cifarm-dev-1" --region "ap-southeast-1"'
                        sh 'kubectl get pods'
                        }
        }
    }
}